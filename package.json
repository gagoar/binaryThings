{
  "name": "binaryThings",
  "version": "1.0.0",
  "description": "some algorithmic implementations of common questions about binary search, balanced trees and such.",
  "main": "index.js",
  "repository": "git@github.com:gagoar/binaryThings.git",
  "author": "gagoar <xeroice@gmail.com>",
  "license": "MIT",
  "eslintConfig": {
    "extends": "eventbrite",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "no-unused-expressions": [
        2,
        {
          "allowTernary": true
        }
      ],
      "import/unambiguous": "off"
    }
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-regenerator",
      "transform-runtime",
      "transform-class-properties",
      "transform-decorators"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        "env",
        "react",
        "stage-0"
      ],
      "plugins": [
        "transform-regenerator",
        "transform-runtime",
        "transform-class-properties",
        "transform-decorators"
      ]
    }
  },
  "dependencies": {
    "ava": "^0.22.0",
    "babel-eslint": "^7.2.3",
    "d3": "^4.10.2",
    "tsc": "^1.20150623.0"
  },
  "devDependencies": {
    "ajv": "^5.2.2",
    "babel-env": "^2.4.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-register": "^6.26.0",
    "eslint": "^3.0.0",
    "eslint-config-eventbrite": "^4.1.0",
    "eslint-plugin-import": "^2.7.0",
    "istanbul": "^0.4.5",
    "jsdom": "^11.2.0",
    "nyc": "^11.2.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.8.1"
  },
  "scripts": {
    "start": "webpack-dev-server --progress",
    "test:debug": "inspect node_modules/ava/profile.js",
    "test": "nyc   ava --verbose",
    "test:watch": "yarn test -- --watch"
  }
}
